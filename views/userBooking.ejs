<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Cyber Cars</title>
</head>
<body>
    <%- include('partials/navbar') %>
    <div class="container">
        <div class="table-data">
            <h2>Bookings</h2>
            <table>
                <tr>
                    <th>Model</th>
                    <th>Color</th>
                    <th>Price</th>
                    <th>Date</th>
                </tr>
                <% for(var i = 0; i < data.length; i++) { %>
                    <tr>
                   <td><a href="/car/<%=data[i].carID%>"><%= data[i].model %></a> </td>
                   <td> <%= data[i].color %> </td>
                    <td> <%= data[i].price %></td>
                    <td> <%= data[i].date %></td>
                    </tr>
                <% } %>
        </table>
        </div>
    </div>
    
</body>
</html>